<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Background Colors-->
    <LinearGradientBrush x:Key="greenBgr1">
        <GradientStop Color="#30ad9a" Offset="0"/>
        <GradientStop Color="#18c274" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="greenBgr2">
        <GradientStop Color="#30ad9a" Offset="1"/>
        <GradientStop Color="#18c274" Offset="0"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="BlackBgrMenu">
        <GradientStop Color="#000000" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="BlackBgrCenter">
        <GradientStop Color="#121212" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="BlackBgrPlayer">
        <GradientStop Color="#181818" />
    </LinearGradientBrush>


    <!--Color Palette-->
    <SolidColorBrush x:Key="color1" Color="#18c274"/>
    <SolidColorBrush x:Key="color2" Color="#FFFAFA"/>
    <SolidColorBrush x:Key="color3" Color="#30ad9a"/>
    <SolidColorBrush x:Key="foreGrSearchBar" Color="#97e6c1"/>
    <SolidColorBrush x:Key="color4" Color="#9ae5c3"/>
    <SolidColorBrush x:Key="color5" Color="#c2e8dc"/>




</ResourceDictionary>